<!-- Banner -->
<section class="bsb-hero-2 px-3">
    <div class="container overflow-hidden">
        <div class="row gy-3 gy-lg-0 align-items-lg-center justify-content-lg-between">
            <div class="col-12 col-lg-6 order-1 order-lg-0">
                <h2 class="display-6 mb-3">Hello, I'm Harpal</h2>
                <p class="fs-1 fw-bold mb-5">Expert in delivering scalable, high-quality solutions aligned with business objectives.</p>
                <div class="d-grid gap-2 d-sm-flex">
                    <a class="btn bsb-btn-2xl btn-dark rounded-pill px-4 fs-6 m-0" asp-area="" asp-controller="About" asp-action="Index" role="button">More about me</a>
                    @* <button type="button" class="btn btn-outline-primary bsb-btn-xl rounded-pill">Buy Credits</button> *@
                </div>
            </div>
            <div class="col-12 col-lg-4 text-center">
                <img class="img-fluid" loading="lazy" src="../assets/images/señor_harpal.webp" alt="Art of Design" style="-webkit-mask-image: url(../assets/images/banner-img-blob-1.svg); mask-image: url(../assets/images/banner-img-blob-1.svg);">
            </div>
        </div>
    </div>
</section>

<!-- About -->
<section class="py-3 py-md-5 bg-light">
    <!--<div class="container mb-4 mb-md-5">
         <div class="row justify-content-md-center">
             <div class="col-12 col-md-10 col-xxl-8">
                 <img class="img-fluid rounded shadow" loading="lazy" src="./assets/img/about-img-1.jpg" alt="About 3">
             </div>
         </div>
     </div>-->

    <div class="container overflow-hidden">
        <div class="row gy-2 gy-md-0 justify-content-xxl-center">
            <div class="col-12 order-md-1 col-md-8 col-xxl-8">
                <div class="text-center text-md-start">
                    <h2 class="display-6 fw-bold lh-1">About me</h2>
                    <p class="text-secondary fs-4 mb-2">SDE 2 Developer</p>
                    <hr class="w-25 mx-auto ms-md-0 mb-4 text-secondary">
                    <p>
                        I am an experienced <span class="fw-bold">Software Development Engineer II (SDE 2)</span>
                        specializing in <span class="fw-bold">.NET</span> technologies,
                        with a proven track record of designing and implementing robust, scalable, and high-performance software solutions.
                    </p>
                    <p>
                        Over the course of my career, I have successfully led the development of complex backend systems, API integrations,
                        and enterprise-level applications, consistently delivering high-quality code that aligns with both technical standards and business objectives.
                    </p>
                    <p>
                        I am adept at collaborating with cross-functional teams, driving project execution from concept through deployment,
                        and continuously optimizing for efficiency and performance.
                    </p>
                </div>
            </div>
            <div class="col-12 order-md-0 col-md-4 col-xxl-4">
                <div class="text-center text-md-start me-md-3 me-xl-5">
                    <p class="mb-4">
                        <span class="d-block display-6 lh-1">5+</span>
                        <span class="fs-4 text-secondary">years of experience</span>
                    </p>
                    <div class="d-grid">
                        <a class="btn bsb-btn-2xl btn-dark rounded-pill px-4 fs-6 m-0" asp-area="" asp-controller="About" asp-action="Index" role="button">Know More</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

