@{
    ViewData["Title"] = "Home";
}

@section Styles {
    <link rel="stylesheet" href="~/css/hero-2.css" />
    <link rel="stylesheet" href="~/css/service-5.css" />
    <link rel="stylesheet" href="~/css/skill-2.css" />
